		<Model BuildPercentUpperBound="0.67" File="Models\Misc\Spotlight_Turret_Large_Constr2.mwm" />
		<Model BuildPercentUpperBound="1.00" File="Models\Misc\Spotlight_Turret_Large_Constr3.mwm" />
	</BuildProgressModels>
